<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.android.testapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.RxAndroid"
            android:label="RxAndroid" />
        <activity android:name=".ui.GlideActivity" />
        <activity android:name=".ui.RxRegForm" />
        <activity android:name=".RetrofitGitHub.ui.GitHubActivity" />
        <activity android:name=".ui.CustomViews" />
        <activity android:name=".ui.FragmentBasicsActivity"></activity>
    </application>

</manifest>